<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.example.recyclerview">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="Zoek Resultaten"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity
			android:name=".AfspraakMaken"
			android:label="Plan een Afspraak"
			android:parentActivityName=".KapperInfo" />
		<activity
			android:name=".AfspraakBeheren"
			android:label="Afspraken Beheren"
			android:parentActivityName=".AfspraakMaken" />
		<activity
			android:name=".KapperInfo"
			android:label="Kapper Informatie"
			android:parentActivityName=".ZoekResultaten" />
		<activity android:name=".ZoekResultaten">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>

</manifest>